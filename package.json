{
  "name": "darna-website",
  "version": "1.0.0",
  "description": "backend darna",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solangeohana/darna-website.git"
  },
  "author": "Solange Ohana",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/solangeohana/darna-website/issues"
  },
  "homepage": "https://github.com/solangeohana/darna-website#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "concurrently": "^5.3.0",
    "content-range": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.5"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ]
  }
}
